 pyproject.toml | 9 ++++++---
 setup.py       | 9 ++++++---
 2 files changed, 12 insertions(+), 6 deletions(-)

diff --git a/pyproject.toml b/pyproject.toml
index 47c25c3..3f6860f 100644
--- a/pyproject.toml
+++ b/pyproject.toml
@@ -4,7 +4,7 @@ build-backend = "setuptools.build_meta"
 
 [project]
 name = "PyFastNER"
-dynamic = ["dependencies", "version"]
+dynamic = ["version"]
 authors = [{name = "Jianlin", email="jianlinshi.cn@gmail.com"}]
 description = "PyFastNER is the python implementation of FastNER, which is orginally developed using Java. It uses hash function to process multiple rules at the same time. Similar to FastNER, PyFastNER supports token-based rules (FastNER--under developing) and character-based rules (FastCNER). It is licensed under the MIT License."
 keywords=['PyFastNER', 'ner', 'regex']
@@ -15,10 +15,13 @@ classifiers = [
     "Programming Language :: Python :: 3",
 ]
 readme='README.md'
+dependencies = [
+    "cython",
+    "quicksectx>=0.3.1",
+]
 
 [tool.setuptools.dynamic]
-dependencies = {file = ["requirements.txt"]}
 version = {attr = "PyFastNER.__version__" }
 
 [project.urls]
-Source = "https://github.com/jianlins/PyFastNER"
\ No newline at end of file
+Source = "https://github.com/jianlins/PyFastNER"
diff --git a/setup.py b/setup.py
index 4883686..e2812f9 100644
--- a/setup.py
+++ b/setup.py
@@ -43,7 +43,7 @@ def parse_requirements(requirements_file):
     return requirements
 
 build_version=get_version()
-install_reqs = parse_requirements('requirements.txt')
+# install_reqs = parse_requirements('requirements.txt')
 
 here = path.abspath(path.dirname(__file__))
 with open(path.join(here, 'README'), encoding='utf-8') as f:
@@ -75,8 +75,11 @@ setup(
         "Topic :: Software Development :: Libraries :: Python Modules",
         "Topic :: Text Processing :: Linguistic",
     ],
-    install_requires=parse_requirements('requirements.txt'),
-    tests_require=parse_requirements('dev-requirements.txt'),
+    install_requires=[
+        "cython",
+        "quicksectx>=0.3.1",
+    ],
+    tests_require=[],
     data_files=[('demo_data', ['conf/crule_test.tsv'])],
     package_data={'': ['*.pyx', '*.pxd']},
     include_dirs=include_dirs
